    <%- include('navbar',{user}); %>
    <div class="header">
      <div>
        <h1>Beverly Springfield</h1>
        <p>2821 Lake Sevilla, Palm Harbor, TX</p>
      </div>
      <div class="buttons">
        <button>Share</button>
